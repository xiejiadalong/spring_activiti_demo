<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.longge.spring.dao.FileMapper">
    
      <insert id="insertFilePath" parameterType="com.longge.spring.entity.FilePathEntity">
         insert into file(id,path,userId,comment)
         values
         (#{id},#{path},#{userId},#{comment})
 
     </insert>
 </mapper>